FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     multiRegionFoamCond1;

startFrom       latestTime;

startTime       0.0;

stopAt          endTime;

endTime         1.0;

deltaT          1e-5;

writeControl    adjustableRunTime;
writeInterval   0.005;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression on;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;


maxCo           0.2;
maxFo           0.5;
maxDi           0.5;
maxDeltaT	0.001;
adjustTimeStep  yes;


functions
{
        
	FluidWallHeatFlux{
            region	fluid;
            patches     ("down");
            name    down;
            type            myWallHeatFlux;
            libs            ("libMyfieldFunctionObjects.so");
            refT	320.735;


            executeControl  outputTime;
            writeControl    outputTime;
            writeInterval   1;	
	}
}


